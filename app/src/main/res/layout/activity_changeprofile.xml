    <EditText
        android:id="@+id/Dob_prof"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:layout_marginStart="24dp"
        android:background="#f0efe0"
        android:ems="10"
        android:hint=" Date of birth"
        android:inputType="date"
        android:minHeight="48dp"
        android:textColor="#433C03"
        android:textColorHint="#4A4304"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Fullname_prof"
        app:layout_constraintVertical_bias="0.033" />

    <Button
        android:id="@+id/updatebtn"
        android:layout_width="165dp"
        android:layout_height="60dp"
        android:layout_below="@+id/ID_prof"
        android:backgroundTint="#eeecc0"
        android:gravity="center"
        android:onClick="update"
        android:text="UPDATE"
        android:textColor="#433C03"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.447"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ID_prof"
        app:layout_constraintVertical_bias="0.357" />
